<?xml version="1.0" encoding="utf-8"?>
<knimeNode icon="./ShapefileReader.png" type="Source" xmlns="http://knime.org/node/v2.10"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://knime.org/node/v2.10 http://knime.org/node/v2.10.xsd">
	<name>Shapefile Reader</name>

	<shortDescription>
		Reads a shapefile.
	</shortDescription>

	<fullDescription>
		<intro>
			This node reads all data from a shapefile. This data is provided in a table with a Shape
			column, that can be used by the visualizer nodes.
		</intro>

		<option name="Shapefile">
			Location of the shapefile (.shp).
		</option>
	</fullDescription>

	<ports>
		<outPort index="0" name="Shape Table">
			Table with Shape column.
		</outPort>
	</ports>
</knimeNode>
