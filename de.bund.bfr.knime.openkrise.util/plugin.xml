<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>
<plugin>

   <!-- This is an example for providing an own category 
      <extension
         point="org.knime.workbench.repository.categories">
      <category
            description="Category containing my nodes"
            icon="icons/default.png"
            level-id="myNodes"
            name="My nodes"
            path="/"/>
   </extension>
   -->
   
   <extension
         point="org.knime.workbench.repository.nodes">
      <node
            category-path="/foodchain"
            factory-class="de.bund.bfr.knime.openkrise.util.cluster.DBSCANNodeFactory"
            id="de.bund.bfr.knime.openkrise.util.cluster.DBSCANNodeFactory">
      </node>
      <node
            category-path="/foodchain"
            factory-class="de.bund.bfr.knime.openkrise.util.geocode.GeocodingNodeFactory"
            id="de.bund.bfr.knime.openkrise.util.geocode.GeocodingNodeFactory">
      </node>
      <node
            category-path="/foodchain"
            factory-class="de.bund.bfr.knime.openkrise.util.shapefilereader.ShapefileReaderNodeFactory"
            id="de.bund.bfr.knime.openkrise.util.shapefilereader.ShapefileReaderNodeFactory">
      </node>
      <node
            category-path="/foodchain"
            factory-class="de.bund.bfr.knime.openkrise.util.tracing.TracingParametersNodeFactory"
            id="de.bund.bfr.knime.openkrise.util.tracing.TracingParametersNodeFactory">
      </node>
   </extension>
   
</plugin>
