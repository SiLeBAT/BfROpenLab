<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<?eclipse version="3.0"?>

<plugin>

   <!-- This is an example for providing an own category 
      <extension
         point="org.knime.workbench.repository.categories">
      <category
            description="Category containing my nodes"
            icon="icons/default.png"
            level-id="myNodes"
            name="My nodes"
            path="/"/>
   </extension>
   -->
   
   <extension point="org.knime.workbench.repository.nodes">
      <node category-path="/community/pvs" factory-class="de.uni_heidelberg.ifi.pvs.knime.octavescriptnode.OctaveScriptNodeFactory" id="de.uni_heidelberg.ifi.pvs.knime.octavescriptnode.OctaveScriptNodeNodeFactory"/>
   </extension>
   <extension point="org.knime.workbench.repository.categories">
      <category icon="icons/octave-logo.png" level-id="pvs" name="Octave Scripting" path="/community">
      </category>
   </extension>
   
</plugin>
